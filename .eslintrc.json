{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb", "prettier", "pugin:node/recommended"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-unused-vars": 0,
    "no-console": 0,
    "indent": 0,
    "linebreak-style": 0,
    "no-process-exit": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 100,
        "tabWidth": 4,
        "semi": true,
        "endOfLine": "auto",
        "parser": "flow"
      }
    ]
  },
  "plugins": ["prettier"]
}
